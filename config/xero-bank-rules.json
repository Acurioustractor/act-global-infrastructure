    "bank_fees": {
      "description": "Bank fees - no receipt required, auto-reconcile",
      "rules": [
        {
          "name": "NAB Account Fee",
          "condition": {
            "field": "Bank Statement Line",
            "contains": ["NAB ACCOUNT FEE", "A/C FEE"]
          },
          "then": {
            "account": "6800 - Bank Fees",
            "tax_rate": "No GST",
            "tracking": "ACT-IN"
          },
          "auto_reconcile": true,
          "no_receipt_required": true
        },
        {
          "name": "NAB International Fee",
          "condition": {
            "field": "Bank Statement Line",
            "contains": ["NAB INT'L", "INTERNATIONAL FEE", "INTL FEE"]
          },
          "then": {
