  "jax_automation": {
    "description": "Xero JAX AI automation settings",
    "enable": true,
    "confidence_threshold": 0.85,
    "learn_from_corrections": true,
    "instructions": [
      "1. Go to Xero → Bank Accounts → Select account",
      "2. Look for 'Automation' or 'JAX' toggle",
      "3. Turn ON automation",
      "4. JAX learns from your manual reconciliations",
      "5. Review suggested matches weekly"
    ]
  },
  "setup_checklist": [
    "1. Log in to Xero: https://go.xero.com",
    "2. Go to Settings → Bank Account → Bank Rules",
    "3. Create rule for each subscription (copy conditions from this config)",
    "4. For subscriptions: Enable auto-reconcile",
    "5. For travel: Disable auto-reconcile, set tracking to 'Ask'",
    "6. For bank fees: Enable auto-reconcile, no receipt required",
    "7. Enable JAX automation on bank accounts",
    "8. Test with recent transactions before enabling batch"
  ]
